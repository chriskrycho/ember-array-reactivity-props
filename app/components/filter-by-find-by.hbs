  <p><code>this.tracked.findByYellow</code>: {{this.tracked.findByYellow}}</p>
  <p class="blue"><code>this.tracked.findYellow</code>: {{this.tracked.findYellow}}</p>
  <button {{on "click" this.tracked.oldPush}}>old push banana</button>
  <button {{on "click" this.tracked.push}}>new push banana</button>
</div>