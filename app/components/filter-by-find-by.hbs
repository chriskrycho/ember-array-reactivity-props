
<div>
  <h2>native array with prototype extensions</h2>
  <p>
    <code>native array</code>
    {{#each this.untracked.items as |item|}}
        {{item.food}}
    {{/each}}
  </p>

  <p><code>this.untracked.filterByFruit</code>: {{this.untracked.filterByFruit}}</p>
  <p class="blue"><code>this.untracked.filterFruit</code>: {{this.untracked.filterFruit}}</p>

  <p><code>this.untracked.findByYellow</code>: {{this.untracked.findByYellow}}</p>
  <p class="blue"><code>this.untracked.findYellow</code>: {{this.untracked.findYellow}}</p>

  <button {{on "click" this.untracked.oldPush}}>old push banana</button>
  <button {{on "click" this.untracked.push}}>new push banana</button>
</div>

<div>
