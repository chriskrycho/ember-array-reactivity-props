  <p><code>this.tracked.mapByFood</code>: {{this.tracked.mapByFood}}</p>
  <p class="blue"><code>this.tracked.mapFood</code>: {{this.tracked.mapFood}}</p>
  <button {{on "click" this.tracked.oldPush}}>old push banana</button>
  <button {{on "click" this.tracked.push}}>new push banana</button>
</div>